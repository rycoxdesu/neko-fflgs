{
  "name": "nekos",
  "version": "0.9.0",
  "description": "Neko's FastFlags Saver",
  "main": "main.js",
  "author": "Neko's Circle",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder",
    "dist:portable": "electron-builder --config portable-config.json",
    "dist:all": "npm run dist && npm run dist:portable"
  },
  "devDependencies": {
    "electron": "^26.2.0",
    "electron-builder": "^24.6.0"
  },
  "build": {
    "appId": "com.nekos.fastflagssaver",
    "productName": "NekoFF",
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*"
    ],
    "win": {
      "target": "nsis",
      "icon": "cute2.ico",
      "artifactName": "nekofflgs.exe"
    }
  },
  "dependencies": {}
}
